public class getCodeNums {

	public Object getCodeNum(Object spliter, Object arg1, Object arg2, Object arg3, Object arg4) throws Exception {
		
		StringBuffer sb = new StringBuffer();
		sb.append(arg1).append(spliter);
		sb.append(arg2).append(spliter);
		sb.append(arg3).append(spliter);
		sb.append(arg4);
		
		return sb.toString();
		
	}
}
